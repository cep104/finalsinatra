 <h1>Edit Whiskey</h1>
 <form method="POST" action="/whiskeys/<%= @whiskey.id %>">
  <input id="hidden" type="hidden" name="_method" value="patch">
<label for="name">Name: </label>
<input type="text" name="name" value="<%=@whiskey.name%>"></p>
<p>
<label for="description">Description: </label>
<input type="textarea" name="description" value="<%=@whiskey.description%>">
</p>
<p>
<label for="distillery">Distilleries: </label>
  <% Distillery.all.each do |distillery| %></br>
    <input id="<%= distillery.name%>" type="checkbox" name="distillerys[]" value="<%= distillery.id %>"><%=distillery.name%>
  <% end %>
</p>
<input type="submit" value="Edit Whiskey"></br></br>

<form>
<button><a href="/whiskeys/<%=@whiskey.id%>">Back</a></button>