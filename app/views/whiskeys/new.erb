<h1>Add a new Whiskey</h1>
<form method="POST" action="/whiskeys">
<p>
<label for="name">Name: </label>
<input type="text" name="name"></p>
<p>
<label for="description">Description: </label>
<input type="textarea" name="description">
</p>
<p>
<label for="distillery">Distilleries: </label>
  <% Distillery.all.each do |distillery| %></br>
    <input id="<%= distillery.name%>" type="checkbox" name="distillerys[]" value="<%= distillery.id %>"><%=distillery.name%>
  <% end %>
</p>
<input type="submit" value="Create Whiskey">

<form>